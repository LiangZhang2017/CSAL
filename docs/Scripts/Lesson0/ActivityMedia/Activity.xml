        <Question id="1" agent="Cristina" text="Type the name you selected and it will appear in the box below. # Press enter when you are done." speech="" media="media/loginPage.html" play="A21">
          <Answers>
            <Answer name="Correct" agent="Cristina" type="Correct" text="Great _user_!" speech="" keys="na" threshold="" play="A22,_user_" />
            <Answer name="JordanQuestion" agent="Jordan" type="JordanQuestion" text="Let's see what else we will need to do to complete the lessons." speech="" keys="na" threshold="" play="B14" />
          </Answers>
        </Question>
      </Questions>
      <Expectations />
      <Misconceptions />
    </TutoringPack>
    <TutoringPack name="TP3">
      <Questions>
        <Question id="1" agent="Cristina" text="_user_, please click one of the answers below when the buttons unlock." speech="" media="media/TrainingMedia1.html" play="_user_,A23">
          <Answers>
            <Answer name="Correct" agent="Cristina" type="Correct" text="This question has no right or wrong answer, # but most questions in other lessons do. # A right answer will be highlighted in green." speech="" keys="na" threshold="" play="A24" />
            <Answer name="JordanUnderstand" agent="Jordan" type="JordanUnderstand" text="How do we know if we are right?" speech="" keys="na" threshold="" play="B15" />
          </Answers>
        </Question>
      </Questions>
      <Expectations />
      <Misconceptions />
    </TutoringPack>
    <TutoringPack name="TP4">
      <Questions>
        <Question id="1" agent="Cristina" text="_user_, read the text below and practice scrolling. # Press Click Here to Continue at the bottom of the passage when you are finished." speech="" media="media/TrainingMedia2.html" play="_user_,A25">
          <Answers>
            <Answer name="Correct" agent="Cristina" type="Correct" text="Nice work!" speech="" keys="na" threshold="" play="A26" />
          </Answers>
        </Question>
      </Questions>
      <Expectations />
      <Misconceptions />
    </TutoringPack>
