<?xml version="1.0" encoding="UTF-8"?>
<AutoTutorScript>
  <Agents>
    <Agent name="Jordan" gender="Male" title="" firstName="Jordan" lastName="" displayName="Jordan">
      <SpeechCan name="PositiveFeedback">
        <Item text="You make this look easy." speech="" play="B1" />
      </SpeechCan>
    </Agent>
    <Agent name="Cristina" gender="Female" title="" firstName="Cristina" lastName="Valdez" displayName="Cristina">
      <SpeechCan name="PositiveFeedback">
        <Item text="Way to go!" speech="" play="A1" />
      </SpeechCan>
    </Agent>
  </Agents>
  <SpeechActs />
  <RigidPacks>
    <RigidPack name="QRP3" usedIn="">
      <Item id="1" agent="Cristina" text="In some of the lessons, you will need to scroll through a passage." speech="" media="media/Background.html" play="A2" />
      <Item id="2" agent="Jordan" text="I think I've done that before." speech="" play="B2" />
    </RigidPack>
    <RigidPack name="RP4" usedIn="">
      <Item id="1" agent="Cristina" text="Use the backspace key if you need to erase anything that you have typed by mistake." speech="" play="A3" />
      <Item id="2" agent="Jordan" text="Good to know! # I make mistakes a lot." speech="" play="B3" />
    </RigidPack>
    <RigidPack name="RP1" usedIn="">
      <Item id="1" agent="Cristina" text="Hello. Welcome to AutoTutor. My name is Cristina Valdez and I will be your instructor." speech="Hello. # Welcome to Autotutor. # My name is Cristina Valdez and I will be your instructor." media="media/Lesson0-IntroTitle.html" play="A4" />
      <Item id="2" agent="Jordan" text="Hi! # I'm Jordan and I will be learning with you." speech="" play="B4" />
      <Item id="3" agent="Cristina" text="Thanks for beginning Autotutor. Autotutor is designed to help you with strategies in reading comprehension." speech="Autotutor is designed to help you with strategies in reading comprehension." media="" play="A5" />
      <Item id="4" agent="Jordan" text="Great! # I am happy you have decided to join me in learning to be a better reader." speech="" play="B5" />
      <Item id="5" agent="Cristina" text="You will need to type on a keyboard when using this program. # Let's watch the following video to see a demonstration." speech="" media="media/Background.html" play="A6" />
    </RigidPack>
    <RigidPack name="RPA" usedIn="">
      <Item id="1" agent="Cristina" text="Or, you can click the scroll button at the bottom of the passage." speech="" play="A7" />
      <Item id="2" agent="Jordan" text="I see." speech="" play="B6" />
    </RigidPack>
    <RigidPack name="Closing" usedIn="">
      <Item id="1" agent="Cristina" text="It is time to get started on the lessons." speech="" play="A8" media="media/Lesson0-IntroTitle.html" />
      <Item id="2" agent="Jordan" text="Will the lessons have instructions to help us know what to do?" speech="" play="B7" />
      <Item id="3" agent="Cristina" text="Yes, Jordan. # Before each activity, I will give you instructions. # You can also watch the Review Videos to get a reminder about the topics that will be covered in the activities." speech="" play="A9" />
      <Item id="4" agent="Jordan" text="Great - let's get to it, _user_!" speech="" play="B8,_user_" />
	   <Item id="5" agent="Cristina" text="You are now finished with the first lesson. # Go ahead and choose another lesson to work on." speech="" play="B8,_user_" media="media/Lesson0-Closing.html" />
    </RigidPack>
    <RigidPack name="QRP2" usedIn="">
      <Item id="1" agent="Cristina" text="Look at the page below. # Most of the questions in the lessons will have multiple answer buttons like the screen you see here." speech="" media="media/TrainingMedia1.html" play="A10" />
      <Item id="2" agent="Jordan" text="So, when the buttons unlock, we will click a button like _user_ did on the last page?" speech="" play="B9,_user_,B10" />
      <Item id="3" agent="Cristina" text="That's right, Jordan. # You will select an answer and the button will change color to show that you selected it. # You will then find out if your answer was correct or incorrect." speech="" play="A11" />
    </RigidPack>
    <RigidPack name="QRP4" usedIn="">
      <Item id="1" agent="Cristina" text="In the video below, you can see another user scrolling through a passage. # You can drag the scroll bar on the right to move through the passage. # Remember that after you click the scroll bar, you must continue to press down on the mouse as you drag the bar up and down." speech="" play="A12" />
      <Item id="2" agent="Jordan" text="That seems familiar." speech="" play="B11" />
    </RigidPack>
    <RigidPack name="RP3" usedIn="">
      <Item id="1" agent="Cristina" text="Let's watch a user type their name." speech="" play="A13" />
    </RigidPack>
    <RigidPack name="RP5" usedIn="">
      <Item id="1" agent="Cristina" text="You can press Enter on the keyboard to submit what you have typed." speech="" play="A14" />
    </RigidPack>
    <RigidPack name="RP6" usedIn="">
      <Item id="1" agent="Cristina" text="Buttons in the program will be locked when Jordan and I are talking to you." speech="" media="media/TrainingMedia0.html" play="A15" />
      <Item id="2" agent="Jordan" text="How will we know when the buttons are unlocked?" speech="" play="B12" />
      <Item id="3" agent="Cristina" text="When a button is unlocked, you will be able to move the mouse over the answer options and the buttons will change color. # You can click on them to select your answer." speech="" play="A16" />
      <Item id="4" agent="Jordan" text="I think I get it. # Can you show me how to do this?" speech="" play="B13" />
    </RigidPack>
    <RigidPack name="RP2" usedIn="">
      <Item id="1" agent="Cristina" text="You probably are using a keyboard that looks similar to this one. # While using this program, you may need to type your name or type to respond to a question." speech="" play="A17" />
    </RigidPack>
    <RigidPack name="QRP1" usedIn="">
      <Item id="1" agent="Cristina" text="Before we go any further, tell us which name you selected previously from the list of names. # We will call you by this name throughout the lessons." speech="" play="A18" />
    </RigidPack>
  </RigidPacks>
  <TutoringPacks>
    <TutoringPack name="TP1">
      <Questions>
        <Question id="1" agent="Cristina" text="Right now, the button is locked. # Try clicking the button when it unlocks." speech="" media="media/TrainingMedia0.html" play="A19">
          <Answers>
            <Answer name="Correct" agent="Cristina" type="Correct" text="Great! # Notice when you click the button, it locks again to secure your answer." speech="" keys="na" threshold="" play="A20" />
          </Answers>
        </Question>
      </Questions>
      <Expectations />
      <Misconceptions />
    </TutoringPack>
    <TutoringPack name="TP2">
